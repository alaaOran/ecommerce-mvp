# Copy this file to .env.local and fill in the values
# These variables are required for local dev and production (Vercel)

# MongoDB (required)
MONGODB_URI="mongodb+srv://<user>:<password>@<cluster>/<db>?retryWrites=true&w=majority"

# Auth (required)
# Use a long, random string (64+ chars) in production
JWT_SECRET="replace-with-a-strong-random-secret"

# Stripe (required if you use checkout/payment)
STRIPE_SECRET_KEY="sk_test_..."

# Example of optional public keys if you later expose publishable keys in the client
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="pk_test_..."
